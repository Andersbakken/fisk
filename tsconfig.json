{
    "compilerOptions": {
        "target": "esnext",
        "newLine": "LF",
        "module": "esnext",
        "moduleResolution": "node",
        "typeRoots": ["./src/typings"],
        "forceConsistentCasingInFileNames": true,
        "pretty": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "downlevelIteration": true,
        "strict": true,
        "sourceMap": true,
        "incremental": true,
        "lib": ["es6"],
        "baseUrl": ".",
        "outDir": "dist/"
    }
}
